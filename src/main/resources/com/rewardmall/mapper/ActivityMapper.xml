<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rewardmall.mapper.ActivityMapper">

    <select id="selectByIsActive" resultType="com.rewardmall.pojo.Activity">
        select * from activities where isActive = #{isActive}
    </select>
</mapper>